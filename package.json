{
  "name": "@dougs-compta/qontojs",
  "version": "0.1.2",
  "description": "Qonto api wrapper for nodejs (javascript/typescript)",
  "keywords": [
    "qonto",
    "qontojs",
    "qontopi",
    "api",
    "transactions",
    "bank",
    "account"
  ],
  "author": "Adrien de Peretti <adrien.deperetti@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/lib"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "format": "prettier 'src/**/*.ts' --ignore-path ./.prettierignore --write",
    "format:check": "prettier 'src/**/*.ts' --ignore-path ./.prettierignore --write --check",
    "clean": "npm run tslint && npm run prettier",
    "build": "npm run format && npm run lint && rm -rf dist && node_modules/.bin/tsc -p tsconfig.build.json",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@types/node": "^10.12.21",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "jest": "^24.1.0",
    "nock": "^10.0.6",
    "prettier": "1.16.4",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dougs-compta/qontojs.git"
  },
  "bugs": {
    "url": "https://github.com/dougs-compta/qontojs/issues"
  },
  "homepage": "https://github.com/dougs-compta/qontojs#readme"
}
